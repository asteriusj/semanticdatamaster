      "part": "Sensors",
      "StillImage": [
        "Device_(SOSA-SSN).png",
        "MeasurementCapability_(SOSA-SSN).png",
        "Record_(SOSA-SSN).png"
      ],
      "modified": "2017-09-14T04:02:00",
