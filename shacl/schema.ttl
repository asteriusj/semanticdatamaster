schema:QualitativeValue
  rdf:type rdfs:Class ;
  rdf:type sh:NodeShape ;
  rdfs:comment "A predefined value for a product characteristic, e.g. the power cord plug type 'US' or the garment sizes 'S', 'M', 'L', and 'XL'."^^rdf:HTML ;
  rdfs:label "Qualitative value" ;
  rdfs:subClassOf schema:Enumeration ;
  sh:property [
      sh:path schema:additionalProperty ;
      sh:class schema:PropertyValue ;
      sh:description """A property-value pair representing an additional characteristics of the entitity, e.g. a product feature or another characteristic for which there is no matching property in schema.org.\\n\\nNote: Publishers should be aware that applications designed to use specific schema.org properties (e.g. http://schema.org/width, http://schema.org/color, http://schema.org/gtin13, ...) will typically expect such data to be provided using those properties, rather than using the generic property/value mechanism.
"""^^rdf:HTML ;
      sh:name "additionalProperty" ;
    ] ;
  sh:property [
      sh:path schema:equal ;
      sh:class schema:QualitativeValue ;
      sh:description "This ordering relation for qualitative values indicates that the subject is equal to the object."^^rdf:HTML ;
      sh:name "equal" ;
    ] ;
  sh:property [
      sh:path schema:greater ;
      sh:class schema:QualitativeValue ;
      sh:description "This ordering relation for qualitative values indicates that the subject is greater than the object."^^rdf:HTML ;
      sh:name "greater" ;
    ] ;
  sh:property [
      sh:path schema:greaterOrEqual ;
      sh:class schema:QualitativeValue ;
      sh:description "This ordering relation for qualitative values indicates that the subject is greater than or equal to the object."^^rdf:HTML ;
      sh:name "greaterOrEqual" ;
    ] ;
  sh:property [
      sh:path schema:lesser ;
      sh:class schema:QualitativeValue ;
      sh:description "This ordering relation for qualitative values indicates that the subject is lesser than the object."^^rdf:HTML ;
      sh:name "lesser" ;
    ] ;
  sh:property [
      sh:path schema:lesserOrEqual ;
      sh:class schema:QualitativeValue ;
      sh:description "This ordering relation for qualitative values indicates that the subject is lesser than or equal to the object."^^rdf:HTML ;
      sh:name "lesserOrEqual" ;
    ] ;
  sh:property [
      sh:path schema:nonEqual ;
      sh:class schema:QualitativeValue ;
      sh:description "This ordering relation for qualitative values indicates that the subject is not equal to the object."^^rdf:HTML ;
      sh:name "nonEqual" ;
    ] ;
  sh:property [
      sh:path schema:valueReference ;
      sh:description "A pointer to a secondary value that provides additional information on the original value, e.g. a reference temperature."^^rdf:HTML ;
      sh:name "valueReference" ;
      sh:or (
          [
            sh:class schema:Enumeration ;
          ]
          [
            sh:class schema:PropertyValue ;
          ]
          [
            sh:class schema:QualitativeValue ;
          ]
          [
            sh:class schema:QuantitativeValue ;
          ]
          [
            sh:class schema:StructuredValue ;
          ]
        ) ;
    ] ;
.
schema:QuantitativeValue
  rdf:type rdfs:Class ;
  rdf:type sh:NodeShape ;
  rdfs:comment " A point value or interval for product characteristics and other purposes."^^rdf:HTML ;
  rdfs:label "Quantitative value" ;
  rdfs:subClassOf schema:StructuredValue ;
  sh:property [
      sh:path schema:additionalProperty ;
      sh:class schema:PropertyValue ;
      sh:description """A property-value pair representing an additional characteristics of the entitity, e.g. a product feature or another characteristic for which there is no matching property in schema.org.\\n\\nNote: Publishers should be aware that applications designed to use specific schema.org properties (e.g. http://schema.org/width, http://schema.org/color, http://schema.org/gtin13, ...) will typically expect such data to be provided using those properties, rather than using the generic property/value mechanism.
"""^^rdf:HTML ;
      sh:name "additionalProperty" ;
    ] ;
  sh:property [
      sh:path schema:maxValue ;
      sh:datatype xsd:float ;
      sh:description "The upper value of some characteristic or property."^^rdf:HTML ;
      sh:name "maxValue" ;
    ] ;
  sh:property [
      sh:path schema:minValue ;
      sh:datatype xsd:float ;
      sh:description "The lower value of some characteristic or property."^^rdf:HTML ;
      sh:name "minValue" ;
    ] ;
  sh:property [
      sh:path schema:unitCode ;
      sh:description "The unit of measurement given using the UN/CEFACT Common Code (3 characters) or a URL. Other codes than the UN/CEFACT Common Code may be used with a prefix followed by a colon."^^rdf:HTML ;
      sh:name "unitCode" ;
      sh:or (
          [
            sh:nodeKind sh:IRI ;
          ]
          [
            sh:datatype xsd:string ;
          ]
        ) ;
    ] ;
  sh:property [
      sh:path schema:unitText ;
      sh:datatype xsd:string ;
      sh:description """A string or text indicating the unit of measurement. Useful if you cannot provide a standard unit code for
<a href='unitCode'>unitCode</a>."""^^rdf:HTML ;
      sh:name "unitText" ;
    ] ;
  sh:property [
      sh:path schema:value ;
      sh:description "The value of the quantitative value or property value node.\\n\\n* For [[QuantitativeValue]] and [[MonetaryAmount]], the recommended type for values is 'Number'.\\n* For [[PropertyValue]], it can be 'Text;', 'Number', 'Boolean', or 'StructuredValue'."^^rdf:HTML ;
      sh:name "value" ;
      sh:or (
          [
            sh:class schema:StructuredValue ;
          ]
          [
            sh:datatype xsd:boolean ;
          ]
          [
            sh:datatype xsd:float ;
          ]
          [
            sh:datatype xsd:string ;
          ]
        ) ;
    ] ;
  sh:property [
      sh:path schema:valueReference ;
      sh:description "A pointer to a secondary value that provides additional information on the original value, e.g. a reference temperature."^^rdf:HTML ;
      sh:name "valueReference" ;
      sh:or (
          [
            sh:class schema:Enumeration ;
          ]
          [
            sh:class schema:PropertyValue ;
          ]
          [
            sh:class schema:QualitativeValue ;
          ]
          [
            sh:class schema:QuantitativeValue ;
          ]
          [
            sh:class schema:StructuredValue ;
          ]
        ) ;
    ] ;
.
