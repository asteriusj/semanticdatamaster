      sh:name "pageEnd" ;
      sh:or (
          [
            sh:datatype xsd:integer ;
          ]
          [
            sh:datatype xsd:string ;
          ]
        ) ;
    ] ;
  sh:property [
      sh:path schema:pageStart ;
      sh:description "The page on which the work starts; for example \"135\" or \"xiii\"."^^rdf:HTML ;
      sh:name "pageStart" ;
      sh:or (
          [
            sh:datatype xsd:integer ;
          ]
